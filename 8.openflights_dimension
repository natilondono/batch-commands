#!/bin/bash
#SBATCH -J openflight_min6_max7
#SBATCH --mail-type=ALL
#SBATCH -e /home/nl26togu/errorfile.err.%j
#SBATCH -o /home/nl26togu/noerror.out.%j
#SBATCH --mem-per-cpu=100000
#SBATCH -t 48:00:00
#SBATCH -n 20
REM The objective is to find Betti Numbers higher than 6
module purge
ml gcc
ml cmake
cd flagser
(mkdir -p build && cd build && cmake .. && make -j)
./test/run
./flagser-memory --approximate 100000 --out openflights_approx ../graphs/flag/openflights_dim0.txt
